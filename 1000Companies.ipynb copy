    "#load dataset and extract the independent and dependent variables\n",
    "companies = pd.read_csv('/Users/macbook/Downloads/1000_Companies.csv')\n",
    "X= companies.iloc[:, :-1].values #this means take every column except for the last column\n",
    "y = companies.iloc[:, 4].values\n",
    "\n",
    "companies.head()"
   ]
