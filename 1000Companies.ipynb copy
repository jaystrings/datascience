    "#piechart to determine the spread of profit made by companies across state\n",
    "# Group profits by company (if not already unique)\n",
    "profit_by_states = companies.groupby(\"State\")[\"Profit\"].sum()\n",
    "\n",
    "# Pie chart\n",
    "plt.figure(figsize=(8,4))\n",
    "plt.pie(\n",
    "    profit_by_states, \n",
    "    labels=profit_by_states.index, \n",
    "    autopct='%1.1f%%',     # show percentage\n",
    "    startangle=140,        # rotate for better view\n",
    "    wedgeprops={\"edgecolor\":\"white\"}  # clean edges\n",
    ")\n",
    "plt.title(\"Profit Distribution Across Companies\")\n",
    "plt.show()\n"
   ]
