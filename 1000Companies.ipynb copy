    "#Data visualization\n",
    "#Building the Correlation matrix\n",
    "numeric_companies = companies.select_dtypes(include=['int64', 'float64'])\n",
    "# Build correlation matrix\n",
    "corr_matrix = numeric_companies.corr()\n",
    "\n",
    "# Plot heatmap\n",
    "sns.heatmap(corr_matrix, annot=True, cmap='coolwarm')\n",
    "plt.show()\n"
   ]
